	month = jan,
	year = {2013},
	note = {Publisher: Emerald Group Publishing Limited},
	keywords = {Conceptual framework, Discrete event simulation, Health care, Hybrid simulation, Hybrid simulation modelling, Methodology, Modelling, Multi‚Äêmethod simulation, Simulation, System dynamics},
	pages = {50--74},
}

