	keywords = {Computational modeling, Computer Society, Computer simulation, Costs, Counting circuits, Project management, Queueing analysis, Software maintenance, Software systems, Stochastic processes},
	pages = {43--58},
}

@techreport{glaiel_agile_2014,
	type = {Working {Paper}},
	title = {Agile {Project} {Dynamics}: {A} {System} {Dynamics} {Investigation} of {Agile} {Software} {Development} {Methods}},
	shorttitle = {Agile {Project} {Dynamics}},
	url = {https://dspace.mit.edu/handle/1721.1/103024},
	abstract = {While Agile software development has many advocates, acceptance in the government and defense sectors has been limited. To address questions of meanings to the term ‚ÄúAgile,‚Äù we examine a range of Agile methods practiced and develop a framework of seven characteristics, which we call the Agile Genome. We gain insight into the dynamics of how Agile development compares to classic ‚Äúwaterfall‚Äù approaches by constructing a System Dynamics model for software projects. The Agile Project Dynamics (APD) model captures each of the Agile genes as a separate component of the model and allows experimentation with combinations of practices and management policies. Experimentation with the APD model is used to explore how different genes work in combination with one another to produce both positive and negative effects. The extensible design of the APD model provides the basis for further study of Agile methods and management practices.},
	language = {en\_US},
	urldate = {2023-04-23},
