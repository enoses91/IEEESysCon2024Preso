@phdthesis{drew_quantitative_nodate,
	address = {United States -- California},
	type = {Ph.{D}.},
	title = {A {Quantitative} {Analysis} of {Organizational} {Complexity} and {ITIL} {Implementation} {Challenges}},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	url = {https://www.proquest.com/docview/2299127852/abstract/5C09AC8ADEF24EDAPQ/1},
	abstract = {When implementing the ITIL framework not all IT organization had the same experience. Thorough this study the challenges that emerged during ITIL implementation projects were investigated. The problem addressed in this study was that organization leaders faced challenges when implementing the ITIL framework. There are limited studies from an organization perspective related to the challenges of implementing the ITIL framework found in current literature. The purpose of this quantitative correlational research design was to better understand the challenges organizations experience when implementing the ITIL framework. The complexity theory provided a framework that was used to investigate and explain the challenges of ITIL implementations. The complexity theory concepts of people and processes interaction, unexpected change, uncertainty, unpredictable behavior, and organizations as complex systems provided a framework that was used to analyze these challenges. This study was conducted by via an online questionnaire, which consisted of 17 rating scale, ordinal and nominal type questions, and was accessible on the Qualtrics survey system. The anonymous online questionnaire was distributed to 9,407 itSMF USA members in the United States through posts on its social media sites. The online questionnaire collected data that provided on the following constructs: complexity level, chaotic behavior, uncertainty and resistance. The online questionnaire was accessible for 3 weeks afterwards, the data was extracted from the Qualtrics system and a bivariate correlational analysis was conducted using the IBM SPSS (version 21) analytics software. The Spearman's correlation coefficient rho was calculated on the 39 of 60 usable responses and identified a correlation between an organization size in terms of the number of the IT staff and the lack of executive sponsorship, a lack of resources (time and people) and lack of funding; the organization size in terms of annual turnover and the culture of resistance; and the organization maturity and the lack of executive sponsorship all had statistical significance thus the null hypotheses was rejected. Future research might focus on expanding the population beyond the itSMF USA association within the United States, or on small and medium sized organizations to determine if the results would be any different.},
	language = {English},
	urldate = {2023-06-04},
	school = {Northcentral University},
	author = {Drew, Ross},
	note = {ISBN: 9781085690584},
	keywords = {ITIL, Information technology Infrastructure Library, It service management, Service delivery, Service management},
}

