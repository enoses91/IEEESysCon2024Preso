@article{ford_dynamic_1998,
	title = {Dynamic modeling of product development processes},
	volume = {14},
	issn = {1099-1727},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/%28SICI%291099-1727%28199821%2914%3A1%3C31%3A%3AAID-SDR141%3E3.0.CO%3B2-5},
