	month = jun,
	year = {2005},
	note = {Publisher: AACE International},
	keywords = {Acceleration, Architecture, Construction, Loss aversion, Mathematical models, Risk management in business, Structural design, and labor productivity, construction claims, delay, disruption},
	pages = {12--15},
}

